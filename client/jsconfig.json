{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "@actions/*": ["./actions/*"],
      "@components/*": ["./components/*"],
      "@containers/*": ["./containers/*"],
      "@reducers/*": ["./reducers/*"],
      "@theme/*": ["./theme/*"],
      "@utils/*": ["./utils/*"]
    }
  },
  "exclude": ["node_modules", "**/node_modules/*", "build"]
}
