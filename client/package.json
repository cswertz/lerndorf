    "country-list": "^2.1.0",
    "file-saver": "^2.0.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.4.2",
    "react-redux": "^7.2.1",
